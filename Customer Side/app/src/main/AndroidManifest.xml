<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tabletop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tabletop"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".DelConfirm"
            android:label="@string/title_activity_del_confirm"
            android:theme="@style/Theme.Tabletop.NoActionBar"></activity>
        <activity
            android:name=".getOtpActivity"
            android:label="@string/title_activity_get_otp"
            android:theme="@style/Theme.Tabletop.NoActionBar" />
        <activity
            android:name=".SuccessActivity"
            android:label="@string/title_activity_success"
            android:theme="@style/Theme.Tabletop.NoActionBar" />
        <activity
            android:name=".PaymentActivity"
            android:label="@string/title_activity_payment"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity
            android:name=".MemuDetail"
            android:label="@string/title_activity_memu_detail"
            android:theme="@style/Theme.Tabletop.NoActionBar" />
        <activity
            android:name=".SelectCategory"
            android:label="@string/title_activity_select_category"
            android:theme="@style/Theme.Tabletop.NoActionBar" />
        <activity
            android:name=".Afterlogin"
            android:label="@string/title_activity_afterlogin"
            android:theme="@style/Theme.Tabletop.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Tabletop.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>